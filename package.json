{
  "name": "hugo-course-publisher",
  "version": "1.0.0",
  "description": "An online course static site publisher using Hugo",
  "repository": "mitodl/hugo-course-publisher",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo server -s site -p 3000 --bind 0.0.0.0",
    "start:webpack": "webpack-dev-server --config src/webpack/webpack.dev.js --hot --host 0.0.0.0",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:pdfjs && npm run build:hugo",
    "build:netlify": "npm run build:webpack && npm run build:pdfjs && npm run build:hugo:netlify",
    "build:preview": "npm run build:webpack && npm run build:pdfjs && npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v",
    "build:hugo:netlify": "hugo -d ../dist -s site -v -b $DEPLOY_PRIME_URL",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:hugo:debug": "hugo -d ../dist -s site -v --templateMetrics --debug",
    "build:pdfjs": "./build-scripts/build-pdfjs.sh",
    "build:webpack": "cross-env NODE_ENV=production webpack --config src/webpack/webpack.prod.js",
    "fmt": "LOG_LEVEL= prettier-eslint --write --no-semi $PWD/'src/**/*.js' $PWD/'src/**/*.scss'",
    "fmt:check": "LOG_LEVEL= prettier-eslint --list-different --no-semi $PWD/'src/**/*.js' $PWD/'src/**/*.scss'",
    "scss_lint": "node ./node_modules/sass-lint/bin/sass-lint.js --verbose --no-exit ./src",
    "test": "./scripts/js_test.sh"
  },
  "license": "BSD-3-CLAUSE",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.6.3",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-mitodl": "^0.0.7",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-mocha": "^6.2.1",
    "eslint-plugin-react": "^7.16.0",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^5.0.2",
    "gulp": "^4.0.2",
    "hugo-bin": "*",
    "imports-loader": "^0.8.0",
    "jsdom": "^15.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier-eslint-cli": "^5.0.0",
    "raf": "^3.4.1",
    "rimraf": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  },
  "engines": {
    "node": "^12.13.0",
    "yarn": "^1.19.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.7.0",
    "bootstrap": "^4.3.1",
    "bootstrap-material-design": "^4.1.2",
    "jquery": "^3.4.1",
    "material-icons": "^0.3.1",
    "offcanvas-bootstrap": "^2.5.2",
    "popper.js": "^1.16.0",
    "prettier-eslint": "^9.0.1",
    "react-bootstrap": "^1.0.0-beta.15",
    "react-hot-loader": "^4.12.17",
    "tippy.js": "^5.2.0"
  }
}
